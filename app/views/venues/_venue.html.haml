%div[venue]
  %p.name
    %strong Name
    = h venue.name
  %p.address
    %strong Address
    = h venue.address
  %p.city
    %strong City
    = h venue.city
  %p.province
    %strong Province
    = h venue.province
  %p.postal_code
    %strong Postal code
    = h venue.postal_code
  %p.country
    %strong Country
    = h venue.country
  %p.phone_number
    %strong Phone number
    = h venue.phone_number
  %p.url
    %strong Url
    = h venue.url
  %p.description
    %strong Description
    = h venue.description
  %p.time_zone
    %strong Time zone
    = h venue.time_zone
  %p.latitude
    %strong Latitude
    = h venue.latitude
  %p.longitude
    %strong Longitude
    = h venue.longitude

  = link_to 'Show', object_path(venue)
  |
  = link_to 'Edit', edit_object_path(venue)
  |
  = link_to 'Destroy', object_path(venue), :confirm => 'Really destroy venue?', :method => :delete
